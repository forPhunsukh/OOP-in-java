
package newpackage;

public class Teacher {
    String name,gender;
    int age;
}


package newpackage;
public class Test {
     public static void main(String[] args) {
        Teacher t1 = new Teacher();
    t1.name = "Forhad";
    t1.age = 23;
    t1.gender="Male";

    System.out.println("Name: "+t1.name);
    System.out.println("Age: "+t1.age);
    System.out.println("Gender: "+t1.gender);
    System.out.print("\n");
    }
}
